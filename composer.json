{
	"name": "omegacode/typo3-theme",
	"type": "typo3-cms-extension",
	"description": "This extension provides a theme for a TYPO3 instance.",
	"homepage": "https://bitbucket.org/omegac0de/oc_theme",
	"license": ["GPL-3.0+"],
	"keywords": [
		"TYPO3",
		"CMS",
		"theme"
	],
	"require": {
		"typo3/cms-core": "^7.6.0 || ^8.7.0",
		"fluidtypo3/vhs": ">=3.0.0,<5.0.99"
	},
	"autoload": {
		"psr-4": {
			"OmegaCode\\OcTheme\\": "Classes/"
		}
	},
	"replace": {
		"oc_theme": "self.version",
		"typo3-ter/oc-theme": "self.version"
	},
	"extra": {
		"typo3/cms": {
			"extension-key": "oc_theme"
		}
	},
	"authors": [
		{
			"name": "Wolf Utz",
			"email": "wpu@hotmail.de"
		}
	]
}